  <mapper namespace="com.simple.basic.test.mapper.TestMapper"><!-- namespace 안에는 구현 인터페이스의 경로가 들어감 -->
  	<select id="getTime" resultType="String"><!-- id에는 추상 메소드명, resultType엔 반환 받을 타입이 들어감 (String, int, map, 객체) -->
